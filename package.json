{
	"name": "createCurrentDirectory",
	"displayName": "Create Current Directory",
	"description": "create file / folder to current open file directory",
	"publisher": "wh1tecat",
	"version": "0.0.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/wh1tecat-nya/create-current-directory.git"
	},
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.createFile",
				"title": "create new file"
			},
			{
				"command": "extension.createFolder",
				"title": "create new folder"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^10.17.0",
		"@types/vscode": "^1.39.0",
		"@typescript-eslint/eslint-plugin": "^2.6.0",
		"@typescript-eslint/parser": "^2.6.0",
		"eslint": "^6.6.0",
		"eslint-config-prettier": "^6.5.0",
		"eslint-plugin-prettier": "^3.1.1",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"prettier": "prettier/prettier",
		"typescript": "rc",
		"vscode-test": "^1.2.0"
	}
}
